Object subclass: #Alumno	instanceVariableNames: 'nombreAlumno notasAlumno'	classVariableNames: ''	package: 'TP0_V2'!!Alumno methodsFor: 'as yet unclassified' stamp: 'LuisParedes 4/20/2020 14:30'!establecerNota: notaNueva	notasAlumno add: notaNueva.! !!Alumno methodsFor: 'as yet unclassified' stamp: 'LuisParedes 4/20/2020 14:15'!establecerNombre: nombreAsignado 	nombreAlumno := nombreAsignado.	! !!Alumno methodsFor: 'as yet unclassified' stamp: 'LuisParedes 4/20/2020 14:30'!promedioNotas	^ notasAlumno average.! !!Alumno methodsFor: 'initialization' stamp: 'LuisParedes 4/20/2020 14:24'!initialize 	nombreAlumno := String new.	notasAlumno := OrderedCollection new.! !Object subclass: #Curso	instanceVariableNames: 'nombreCurso dicAlumnos'	classVariableNames: 'minPromedioAprobacion'	package: 'TP0_V2'!!Curso methodsFor: 'as yet unclassified' stamp: 'LuisParedes 4/20/2020 14:38'!promedioDe: padronAlumno	|alumno|	alumno := dicAlumnos at: padronAlumno.		^ (alumno promedioNotas)	! !!Curso methodsFor: 'as yet unclassified' stamp: 'LuisParedes 4/20/2020 16:10'!aprobo: padronAlumno 	|alumno |	alumno := dicAlumnos at: padronAlumno.		^(alumno promedioNotas >= minPromedioAprobacion)	! !!Curso methodsFor: 'as yet unclassified' stamp: 'LuisParedes 4/20/2020 14:14'!establecerNombre: nombreAsignado	nombreCurso := nombreAsignado! !!Curso methodsFor: 'as yet unclassified' stamp: 'LuisParedes 4/20/2020 14:10'!inscribirAlumno: alumnoNuevo conNumeroDePadron: padronAlumno	dicAlumnos at: padronAlumno put: alumnoNuevo.! !!Curso methodsFor: 'as yet unclassified' stamp: 'LuisParedes 4/20/2020 14:29'!calificarA: padronAlumno conNota: notaNueva	|alumno|	alumno := dicAlumnos at: padronAlumno.	alumno establecerNota: notaNueva.! !!Curso methodsFor: 'initialization' stamp: 'LuisParedes 4/20/2020 16:11'!initialize 	nombreCurso := String new.	dicAlumnos := Dictionary new.		minPromedioAprobacion := 4.! !